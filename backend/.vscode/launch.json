    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "env":{
                "HTTPS_PROXY":"",
                "HTTP_PROXY":""
            }
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env":{
                "HTTPS_PROXY":"",
                "HTTP_PROXY":""
            }
        }
    ]
}