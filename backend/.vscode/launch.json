    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "env":{
            }
        },
        {
            "name": "Launch Package ",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env":{
            }
        },
        {
            "name": "Launch Package (proxy)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env":{
                "HTTPS_PROXY":"https://proxy:3012",
                "HTTP_PROXY":"http://proxy:3010",
            }
        }
    ]
}